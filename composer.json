{
    "name": "tolan/usiref-networking",
    "description": "Component for networking.",
    "version": "0.0.2",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "sort-packages": true,
    "authors": [
        {
            "name": "Martin Kovar",
            "email": "mkovar86@gmail.com"
        }
    ],
    "scripts": {
        "cs": "./vendor/bin/phpcs -s",
        "unit": "./vendor/bin/phpunit",
        "coverage": "mkdir -p output && ./vendor/bin/phpunit --coverage-html output"
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.0",
        "squizlabs/php_codesniffer": "3.2.2"
    },
    "autoload": {
        "psr-4": {
            "uSIreF\\Networking\\": "src",
            "uSIreF\\Networking\\Unit\\": "tests\\Unit"
        }
    }
}